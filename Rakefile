desc 'test simplecov with Travis'
task :bruteforce do
  COMMITS.reverse.each do |commit|
    Dir.chdir('simplecov') { sh "git checkout #{commit}" }
    sh "git add ."
    sh "git commit -m 'testing #{commit}'"
    sh "git push"
  end
end
